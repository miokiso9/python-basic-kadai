        "#配列の定義\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# arrayに配列の要素を代入し、出力する\n",
        "for array in array:\n",
        "  print(array)\n",
        "\n",
        "#配列・変数の定義\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "i = 0\n",
        "\n",
        "# 変数が配列の要素数に達するまで、配列の内容を順に出力する\n",
        "while i < 9:\n",
        "  print(array[i])\n",
        "  i +=1"

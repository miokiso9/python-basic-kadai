      "source": [
        "def cal_price(price, consumption_tax):\n",
        "    # 与えられた引数priceと引数consumption_taxを加算し、変数totalに代入する\n",
        "    total = price + price*consumption_tax/100\n",
        "\n",
        "    # 変数$totalの値を出力する\n",
        "    return(total)\n",
        "\n",
        "# 関数を呼び出し、引数として商品金額と消費税を渡す\n",
        "cal_price(110, 10)"
      ]
